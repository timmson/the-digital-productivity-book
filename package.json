  "license": "Private",
  "bugs": {
    "url": "https://github.com/tsidukciya/tsidukciya.github.io/issues"
  },
  "homepage": "https://github.com/tsidukciya/tsidukciya.github.io#readme",
  "devDependencies": {
    "@cspell/dict-ru_ru": "^2.2.4",
    "cspell": "^8.18.1",
    "markdownlint-cli": "0.46.0",
    "tsx": "^4.19.3",
    "xmlbuilder2": "^3.1.1"
  },
  "dependencies": {
    "marked": "^15.0.11",
    "typescript": "^5.8.3"
  }
}
