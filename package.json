{
  "name": "tsidukciya.github.io",
  "version": "1.0.0",
  "description": "For book writing",
  "main": "index.js",
  "private": true,
  "scripts": {
    "lint": "npx markdownlint-cli docs/*.md --fix",
    "spell": "npx cspell docs/*.md --cache --show-suggestions --no-progress",
    "check": "npm run lint && npm run spell",
    "build": "npx tsx ./src/generate-fb2.tsx",
    "start": "npm run check && npm run build"
  },
